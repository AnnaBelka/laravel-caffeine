{
    "name": "genealabs/laravel-caffeine",
    "description": "Keeping Your Laravel Forms Awake",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Bronner",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.0 - 5.5",
        "illuminate/routing": "5.0 - 5.5",
        "jenssegers/model": "*"
    },
    "require-dev": {
        "laravel/laravel": "5.5.*",
        "mockery/mockery": "0.9.*",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "5.7.*",
        "satooshi/php-coveralls" : "*",
        "sebastian/phpcpd": "*",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelCaffeine\\": "src/",
            "GeneaLabs\\LaravelCaffeine\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelCaffeine\\Providers\\Service"
            ]
        }
    }
}
